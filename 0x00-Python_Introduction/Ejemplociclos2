#ejemplo while2

def imp_pares(npares):
    numero=2 #número a imprimir
    pares_impresos=0 #número de pares impresos
    while pares_impresos<npares:
        print(numero)
        pares_impresos+=1
        numero+=2
imp_pares(4)

